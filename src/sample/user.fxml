      <Label layoutX="120.0" layoutY="82.0" prefHeight="25.0" prefWidth="88.0" text="USERNAME" />
      <Label layoutX="118.0" layoutY="152.0" prefHeight="17.0" prefWidth="72.0" text="PASSWORD" />
      <PasswordField fx:id="userPasswordField" layoutX="226.0" layoutY="148.0" />
      <TextField fx:id="usernameField" layoutX="226.0" layoutY="82.0" />
      <Button layoutX="278.0" layoutY="281.0" mnemonicParsing="false" onAction="#backToMainScene" text="BACK" />
   </children>
</AnchorPane>
